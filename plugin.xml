<?xml version="1.0" encoding="UTF-8"?>

<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
  xmlns:android="http://schemas.android.com/apk/res/android"
  id="kunder-cordova-plugin-openAppHandlerUrl"
  version="0.1.0">
  <name>kunder-cordova-plugin-openAppHandlerUrl</name>
  <description>This cordova plugin allows to handle urls with openHandleUrl</description>
  <author>kunder</author>
  <license>MIT</license>
  <keywords>handler, android, ios</keywords>

  <engines>
    <engine name="cordova" version=">=3.0.0" />
  </engines>

  <js-module src="www/openAppHandlerUrl.js" name="openAppHandlerUrl">
    <clobbers target="openAppHandlerUrl" />
  </js-module>

  <asset src="www" target="openAppHandlerUrl" />
</plugin>
